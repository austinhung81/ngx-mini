{
  "name": "ngx-mini",
  "version": "0.0.0",
  "description": "Web UI components for Angular",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --prod && ng test @ngx-lite/all --watch=false && npm run package",
    "test": "ng test @ngx-mini/all",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "genlib": "ng generate library --entry-file index.ts --prefix nm",
    "package": "run-s package:*",
    "package:loader": "ng build @ngx-mini/loader",
    "package:navbar": "ng build @ngx-mini/navbar",
    "publish:lib": "npm publish ./dist/lib"
  },
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "ng2",
    "ngx",
    "ui",
    "components"
  ],
  "author": "Wilson Su <sweekson@yahoo.com.tw>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sweekson/ngx-mini.git"
  },
  "bugs": {
    "url": "https://github.com/sweekson/ngx-mini/issues"
  },
  "homepage": "https://github.com/sweekson/ngx-mini#readme",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "dependencies": {
    "@angular-contrib/common": "0.0.2",
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.3.0",
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/http": "^6.0.6",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "@ngx-prism/core": "^2.0.1",
    "@types/classnames": "^2.2.4",
    "@types/prismjs": "^1.9.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "html-format": "^1.0.1",
    "lodash-es": "^4.17.10",
    "ng-zorro-antd": "^1.0.0",
    "prismjs": "^1.9.0",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.6",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.8",
    "ng-packagr": "^3.0.3",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.1.2",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
