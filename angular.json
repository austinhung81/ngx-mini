{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "components",
  "projects": {
    "ngx-mini-doc": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-mini-doc",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "extractCss": true,
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/prismjs/themes/prism.css",
              "node_modules/ng-zorro-antd/src/ng-zorro-antd.min.css",
              "components/styles/index.scss",
              "src/styles/index.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-mini-doc:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "ngx-mini-doc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-mini-doc:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-mini/all": {
      "root": "components",
      "sourceRoot": "components",
      "projectType": "library",
      "prefix": "nm",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true
        }
      },
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "components/test.ts",
            "tsConfig": "components/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "components/tsconfig.lib.json",
              "components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-mini/host": {
      "root": "components/host",
      "sourceRoot": "components/host/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "components/host/ng-package.json",
            "tsConfig": "components/tsconfig.lib.json"
          },
          "configurations": {
            "production": {
              "project": "components/host/ng-package.prod.json"
            }
          }
        }
      }
    },
    "@ngx-mini/button": {
      "root": "components/button",
      "sourceRoot": "components/button/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "components/button/ng-package.json",
            "tsConfig": "components/tsconfig.lib.json"
          },
          "configurations": {
            "production": {
              "project": "components/button/ng-package.prod.json"
            }
          }
        }
      }
    },
    "@ngx-mini/loader": {
      "root": "components/loader",
      "sourceRoot": "components/loader/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "components/loader/ng-package.json",
            "tsConfig": "components/tsconfig.lib.json"
          },
          "configurations": {
            "production": {
              "project": "components/loader/ng-package.prod.json"
            }
          }
        }
      }
    },
    "@ngx-mini/dropdown": {
      "root": "components/dropdown",
      "sourceRoot": "components/dropdown/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "components/dropdown/ng-package.json",
            "tsConfig": "components/tsconfig.lib.json"
          },
          "configurations": {
            "production": {
              "project": "components/dropdown/ng-package.prod.json"
            }
          }
        }
      }
    },
    "@ngx-mini/navbar": {
      "root": "components/navbar",
      "sourceRoot": "components/navbar/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "components/navbar/ng-package.json",
            "tsConfig": "components/tsconfig.lib.json"
          },
          "configurations": {
            "production": {
              "project": "components/navbar/ng-package.prod.json"
            }
          }
        }
      }
    },
    "ngx-mini-e2e": {
      "root": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ngx-mini-doc:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-mini-doc:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-mini-doc",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}